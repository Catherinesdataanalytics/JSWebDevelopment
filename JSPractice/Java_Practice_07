class Student{
    public int id;
    public Student(int id){
        this.id = id;
    }
}



public class Class {
    public Student[] students;
    
    public Class(int n){
        students = new Student[n];
        
        //
        for (int i = 0; i < n; ++i){
            students[i] = new Student(i);
        }
    }
}
//Data Structure
